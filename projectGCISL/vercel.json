{
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/register", "dest": "api/index.js" },
      { "src": "/api/login", "dest": "api/index.js" },
      { "src": "/api/admin-dashboard", "dest": "api/index.js" },
      { "src": "/api/volunteer-dashboard", "dest": "api/index.js" }
    ]
}  